{
  "name": "codingsimply-gatsby",
  "description": "Coding Simply as a Gatsby Site.",
  "version": "1.0.1",
  "private": true,
  "author": "Adam Saladino<adam@codingsimply.com>",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf public",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "dev": "gatsby develop",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "sass:watch": "node-sass -w assets/scss  -o src/styles",
    "sass:build": "node-sass assets/scss -o src/styles --source-map true",
    "start": "npm run dev"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@emotion/core": "^11.0.0",
    "@emotion/styled": "^11.11.0",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "axios": "^1.6.8",
    "babel-plugin-styled-components": "^2.1.4",
    "classnames": "^2.5.1",
    "css-loader": "^6.10.0",
    "foundation-sites": "^6.8.1",
    "gatsby": "^5.13.3",
    "gatsby-cli": "^5.13.2",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-canonical-urls": "^5.13.1",
    "gatsby-plugin-emotion": "^8.13.1",
    "gatsby-plugin-mdx": "^5.13.1",
    "gatsby-plugin-react-helmet": "^6.13.1",
    "gatsby-plugin-sharp": "^5.13.1",
    "gatsby-plugin-typescript": "^5.13.1",
    "gatsby-remark-copy-linked-files": "^6.13.1",
    "gatsby-remark-images": "^7.13.1",
    "gatsby-remark-prismjs": "^7.13.1",
    "gatsby-remark-responsive-iframe": "^6.13.1",
    "gatsby-remark-smartypants": "^6.13.1",
    "gatsby-source-filesystem": "^5.13.1",
    "gatsby-transformer-json": "^5.13.1",
    "gatsby-transformer-remark": "^6.13.1",
    "gatsby-transformer-sharp": "^5.13.1",
    "highlight.js": "^11.9.0",
    "lodash": "^4.17.21",
    "modern-normalize": "^2.0.0",
    "moment": "^2.30.1",
    "node-sass": "^9.0.0",
    "normalize.css": "^8.0.1",
    "polished": "^4.3.1",
    "postcss-loader": "^8.1.1",
    "postscribe": "^2.0.8",
    "prism-themes": "^1.9.0",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-highlight": "^0.15.0",
    "react-slick": "^0.30.2"
  },
  "devDependencies": {
    "typescript": "^5.4.3",
    "@types/classnames": "^2.3.1",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "@types/react-helmet": "^6.1.11",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "gh-pages": "^6.1.1",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5"
