{
  "name": "codingsimply-gatsby",
  "description": "Coding Simply as a Gatsby Site.",
  "version": "1.0.0",
  "private": true,
  "author": "Adam Saladino<adam@codingsimply.com>",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf public",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "dev": "gatsby develop",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "sass:watch": "node-sass -w assets/scss  -o src/styles",
    "sass:build": "node-sass assets/scss -o src/styles --source-map true",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "npm run dev",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.12",
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-brands-svg-icons": "^5.7.2",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@mdx-js/mdx": "^1.5.0",
    "@mdx-js/react": "^1.5.0",
    "axios": "^0.19.0",
    "babel-plugin-styled-components": "^1.10.1",
    "classnames": "^2.2.6",
    "css-loader": "^3.2.0",
    "foundation-sites": "^6.5.3",
    "gatsby": "^2.10.0",
    "gatsby-cli": "^2.9.0",
    "gatsby-image": "^2.2.1",
    "gatsby-plugin-canonical-urls": "^2.1.0",
    "gatsby-plugin-emotion": "^4.1.0",
    "gatsby-plugin-mdx": "^1.0.47",
    "gatsby-plugin-react-helmet": "^3.1.0",
    "gatsby-plugin-sharp": "^2.2.1",
    "gatsby-plugin-typescript": "^2.1.0",
    "gatsby-remark-copy-linked-files": "^2.1.0",
    "gatsby-remark-images": "^3.1.0",
    "gatsby-remark-prismjs": "^3.3.0",
    "gatsby-remark-responsive-iframe": "^2.2.0",
    "gatsby-remark-smartypants": "^2.1.0",
    "gatsby-source-filesystem": "^2.1.0",
    "gatsby-transformer-json": "^2.2.0",
    "gatsby-transformer-remark": "^2.5.0",
    "gatsby-transformer-sharp": "^2.2.0",
    "highlight.js": "^9.15.10",
    "lodash": "^4.17.15",
    "modern-normalize": "^0.5.0",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "normalize.css": "^8.0.1",
    "polished": "^3.4.1",
    "postcss-loader": "^3.0.0",
    "postscribe": "^2.0.8",
    "prism-themes": "^1.1.0",
    "prismjs": "^1.16.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-highlight": "^0.12.0",
    "react-slick": "^0.25.2",
    "typescript": "^3.5.2"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.8",
    "@types/node": "^12.0.10",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.8",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "eslint": "^6.0.0",
    "eslint-config-blvd": "^0.1.1",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "gh-pages": "^2.0.1",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3"
  }
}
